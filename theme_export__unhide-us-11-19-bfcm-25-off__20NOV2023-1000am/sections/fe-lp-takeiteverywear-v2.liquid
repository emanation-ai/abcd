    {% schema %}
    {
        "name": "Take it everywear v2",
        "settings":[
            {
                "type": "text",
                "id": "takeiteverywear-heading",
                "label": "Heading",
                "default": "Take it everywear"
            },
            {
                "type": "text",
                "id": "takeiteverywear-desc",
                "label": "Description",
                "default": "Lorem ipsum dolor sit amet, consectetuer adipiscing elit, sed diam nonummy nibh euismod tincidunt."
            },
