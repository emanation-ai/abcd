                {
                    "type": "text",
                    "id": "takeiteverywear-heading",
                    "label": "Heading",
                    "default": "Bringing Comfy to A Whole New Level"
                },
                {
                "type": "image_picker",
                "id": "takeiteverywear-image",
                "label": "Image"
