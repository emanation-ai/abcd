<sticky-header class="header-wrapper py-{{section.settings.br}} header-position-{{section.settings.header_position}} ">
  <header class="header header--{{ section.settings.logo_position }} search-{{section.settings.search_style}} page-width {% if section.settings.menu != blank %} header--has-menu{% endif %} py-{{section.settings.page_width}} py-{{section.settings.page_y_pd}} py-{{section.settings.page_x_pd}}">
    {%- if section.settings.menu != blank -%}
      <header-drawer>
        <details class="menu-drawer-container">
          <summary class="header__icon header__icon--menu header__icon--summary link link--text focus-inset" aria-label="{{ 'sections.header.menu' | t }}">
            <span>
              {% render 'icon-hamburger' %}
              {% render 'icon-close' %}
            </span>
          </summary>
          <div id="menu-drawer" class="menu-drawer motion-reduce" tabindex="-1">
            <div class="menu-drawer__inner-container">
              <div class="menu-drawer__navigation-container">
                 <nav class="menu-drawer__navigation">
                  <ul class="menu-drawer__menu list-menu" role="list">
                    {% assign link_count = 0 %}
                    {%- for link in section.settings.menu.links -%}                      
                        {%- if link.links != blank -%}
                          <li class="menu-accordion-list"  data-index="{{ forloop.index }}">
                            {% assign link_count = link_count | plus: 1 %}
                            <details {% if link_count == 1 %}open1{% endif %}>
                              <summary class="list-menu__item link link--text focus-inset{% if link.child_active %} menu-drawer__menu-item--open{% endif %}">
                               <!--  {{ link.title }} -->
                               {% if link.title == "Bundles"%}Bundles{% else %}Blankets{% endif %}
                                
                                {% render 'icon-plus' %}
                                {% render 'icon-minus' %}
                              </summary>
                              <div id="link-{{ link.title | escape }}" class="motion-reduce " tabindex="-1">
                                <div class="menu-drawer__inner-submenu">  
                                  <ul class="list-menu main-sub-menus">
                                    {% comment %}
                                    <div class="drawer-sub-mega-images-wraper">
                                      <div class="drawer-sub-mega-images-inner-wraper">
                                      {%- if section.blocks.size > 0 -%}
                                        {% assign sub_mega_blocks = 0 %}
                                        {%- for block in section.blocks -%}
                                          {%- liquid 
                                            assign name = block.settings.name | strip | downcase
                                            assign title = link.title | strip | downcase 
                                          -%}      
                                    
                                          {% if block.type == 'sub_mega_menu'%}
                                            {% for childlink in link.links %}
                                              {% if childlink.title == block.settings.name %}
                                                {% assign sub_mega_blocks = sub_mega_blocks | plus: 1 %}
                                                <div class="drawer-sub-mega-image-col {{block.settings.img_col_position}}" data-target="{{ block.settings.name | downcase }}">
                                                  
                                                    {%- liquid   
                                                      assign image = block.settings.image_1
                                                      assign title = block.settings.title_1
                                                      assign description = block.settings.description_1
                                                      assign url = block.settings.link_1                  
                                                    -%}                                      
                                                    
                                                    {%- if image != blank -%}
                                                      <div class="drawer-sub-mega-menu-image left-col">
                                                        {%- if url != blank -%}
                                                          <a href="{{ url }}">
                                                        {%- endif -%}
                                                          {%- render 'resimg', image: image -%}
                                                          <div class="short-info-wraper">                                            
                                                            <h3 class="bc-mega-menu-image__title ">{{ title }}</h3> 
                                                            <p class="bc-mega-menu-image__desc ">{{ description }}</p>                                            
                                                          </div>
                                                        {%- if url != blank -%}
                                                          </a>
                                                        {%- endif -%}
                                                      </div>
                                                    {%- endif -%}                 
                                                </div>
                                                {% break %}
                                              {% endif %}
                                            {% endfor %}
                                          {% endif %}
                                    
                                        {%- endfor -%}
                                      {%- endif -%}
                                      </div>
                                    </div>
